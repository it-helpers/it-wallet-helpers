{
    "schema": "https://json-schema.org/draft/2020-12/schema#",
    "title": "federation_entity",
    "type": "object",
    "required": [
        "organization_name",
        "homepage_uri",
        "policy_uri",
        "tos_uri",
        "logo_uri"
    ],
    "properties": {
        "organization_name": {
            "type": "string"                            
        },
        "contacts": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "string"
            }
        },
        "homepage_uri": {
            "type": "string"
        },
        "logo_uri": {
            "type": "string"
        },
        "tos_uri": {
            "type": "string"
        },
        "policy_uri": {
            "type": "string"
        }
    }
}